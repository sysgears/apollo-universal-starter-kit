{
  "name": "apollo-universal-starter-kit",
  "private": true,
  "version": "1.0.0",
  "main": "src/server",
  "engines": {
    "node": ">= 6.11.1",
    "npm": ">= 3.10.3",
    "yarn": ">= 0.19.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production spin build",
    "exp": "spin exp",
    "start": "node --harmony build/server",
    "tests":
      "cross-env NODE_ENV=test PORT=7070 spin test -t 5000 --full-trace --exit \"src/**/*.spec.ts\"",
    "tests:watch":
      "cross-env NODE_ENV=test PORT=7070 spin test -t 5000 --full-trace --watch \"src/**/*.spec.ts\"",
    "test": "npm run tests && npm run lint",
    "lint":
      "tslint --fix 'src/*.ts*' -p tsconfig.json --type-check && tslint --fix 'tools/*.ts*' -p tsconfig.json --type-check",
    "seed": "npm run migrate && node_modules/.bin/knex seed:run",
    "migrate": "ts-node node_modules/.bin/knex migrate:latest",
    "rollback": "ts-node node_modules/.bin/knex migrate:rollback",
    "watch": "spin watch",
    "heroku-postbuild": "rm -rf build && npm run build && npm run seed",
    "cli": "node tools/cli",
    "precommit": "lint-staged",
    "postinstall": "rm -rf node_modules/@types/async"
  },
  "lint-staged": {
    "*.{js,jsx,json}": ["prettier --write", "git add"]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sysgears/apollo-universal-starter-kit.git"
  },
  "bugs": {
    "url": "https://github.com/sysgears/apollo-universal-starter-kit/issues"
  },
  "homepage": "https://github.com/sysgears/apollo-universal-starter-kit",
