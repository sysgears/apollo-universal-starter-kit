    "react-apollo": "^2.1.4",
    "react-debounce-input": "^3.1.0",
    "react-i18next": "^7.6.0",
    "react-native": "0.55.3",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-native-modal": "^5.4.0",
    "react-native-simple-picker": "^2.0.0",
