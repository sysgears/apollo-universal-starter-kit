  "engines": {
    "node": ">= 6.11.1",
    "npm": ">= 3.10.3",
    "yarn": ">= 0.19.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production spin -v build",
    "exp": "spin exp",
    "start": "node --harmony build/server",
    "tests": "cross-env NODE_ENV=test PORT=7070 spin -v test -t 5000 --full-trace --exit \"src/**/*.spec.ts\"",
    "tests:watch": "cross-env NODE_ENV=test PORT=7070 spin test -t 5000 --full-trace --watch \"src/**/*.spec.ts\"",
    "test": "npm run tests && npm run lint",
    "lint": "tslint --fix 'src/*.ts*' -p tsconfig.json --type-check && tslint --fix 'tools/*.ts*' -p tsconfig.json --type-check",
    "seed": "npm run migrate && knex seed:run",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "watch": "spin -v watch",
    "heroku-postbuild": "rm -rf build && npm run build && npm run seed",
    "cli": "node tools/cli"
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
